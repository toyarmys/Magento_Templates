<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <title>Contactdadadada Us</title>
    </head>
    <body>
        <referenceBlock name="contactForm">
            <arguments>
                <argument name="template" xsi:type="string">
                    Magento_Contact::form.phtml
                </argument>
            </arguments>
        </referenceBlock>
        <referenceBlock name="header.content">
            <arguments>
                <argument name="template" xsi:type="string">
                   Magento_Contact::abc.phtml
                </argument>
            </arguments>
        </referenceBlock>
        <referenceContainer name="footer">
            <block class="Magento\Framework\View\Element\Template" name="social.links" template="Magento_Theme::social-links.phtml" after="-" />
        </referenceContainer>
    </body>
</page>
